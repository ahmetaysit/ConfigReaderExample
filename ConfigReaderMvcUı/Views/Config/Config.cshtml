@model ConfigModel

@{
    ViewData["Title"] = "Config";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Config</h2>

<p>
    <a asp-action="Add">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Configs) {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Value</td>
            <td>@item.Type</td>
            <td>@item.ApplicationName</td>
            <td>
                @Html.ActionLink("Edit", "Edit", item) |
                @Html.ActionLink("Details", "Details", item) |
                @Html.ActionLink("Delete", "Delete", item)
            </td>
        </tr>
}
    </tbody>
</table>
